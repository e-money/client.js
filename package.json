    "url": "https://github.com/e-money/client.js/issues"
  },
  "homepage": "https://e-money.com",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "ts-proto": "^1.83.0",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@cosmjs/amino": "^0.26.0",
