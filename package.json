{
  "name": "@e-money/client.js",
  "version": "0.2.2",
  "description": "JavaScript client library for e-Money.com",
  "keywords": [
    "e-money",
    "stablecoin",
    "cryptocurrency",
    "blockchain",
    "wallet",
    "dex"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/",
    "docs/",
